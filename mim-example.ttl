  mim:unidirectioneel true ;
  mim:aggregatietype mim:Geen ;
  mim:kardinaliteit  "0..*" ;
  mim:attribuut      ex:ingangsdatum ;
  mim:datumOpname    "2025-09-05"^^xsd:date
.

ex:ingangsdatum a mim:Attribuutsoort ;
  mim:identificatie  "http://example.org/mim-demo#ingangsdatum"^^xsd:anyURI ;
