@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix mim:  <http://modellen.geostandaarden.nl/def/mim#> .
@prefix ex:   <http://example.org/mim-demo#> .

#################################################################
# Informatiemodel en Package
#################################################################

ex:IMDemo a mim:Informatiemodel ;
  mim:basisUri            "http://example.org/mim-demo#"^^xsd:anyURI ;
  mim:identificatie       "http://example.org/mim-demo#IMDemo"^^xsd:anyURI ;
  mim:naam                "IM Demo"@nl ;
  mim:alias               "Demomodel"@nl ;
  mim:informatiemodeltype skos:Concept ;         # placeholder; in echt: concept uit mim:Informatiemodeltypen
  mim:informatiedomein    "Voorbeeld domein" ;
  mim:relatiemodelleringstype skos:Concept ;     # placeholder; in echt: concept uit mim:Relatiemodelleringstypen
  mim:mimversie           "1.2" ;
  mim:mimtaal             "nl" ;
  mim:tekstopmaak         "markdown" ;
  mim:datumOpname         "2025-09-05"^^xsd:date ;
  mim:bevatModelelement   ex:PkgBasis
.

ex:PkgBasis a mim:Package ;
  mim:basisUri       "http://example.org/mim-demo#"^^xsd:anyURI ;
  mim:identificatie  "http://example.org/mim-demo#PkgBasis"^^xsd:anyURI ;
  mim:naam           "Basis"@nl ;
  mim:alias          "Core"@nl ;
  mim:bevatModelelement ex:Persoon, ex:Adres, ex:woontOp, ex:RelKlasseVoorbeeld,
                        ex:Persoonsgegevens, ex:PersoonsgegevensType,
                        ex:Geslacht, ex:GeslachtM, ex:GeslachtV,
                        ex:Tekst255, ex:Postcode, ex:Datum,
                        ex:LandcodeLijst, ex:LandcodeElementNL, ex:LandcodeElementBE,
                        ex:SubTypePersoon, ex:GeneralisatiePersoon, ex:KeuzeContact
.

#################################################################
# (Primitieve) Datatypen
#################################################################

ex:Tekst255 a mim:PrimitiefDatatype ;
  mim:identificatie  "http://example.org/mim-demo#Tekst255"^^xsd:anyURI ;
  mim:naam           "Tekst255"@nl ;
  mim:definitie      "Vrije tekst tot 255 tekens."@nl ;
  mim:lengte         "255"
.

ex:Datum a mim:PrimitiefDatatype ;
  mim:identificatie  "http://example.org/mim-demo#Datum"^^xsd:anyURI ;
  mim:naam           "Datum"@nl ;
  mim:definitie      "Kalenderdatum (YYYY-MM-DD)."@nl ;
  mim:patroon        "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
.

# Gestructureerd datatype voor Postcode
ex:Postcode a mim:GestructureerdDatatype ;
  mim:identificatie  "http://example.org/mim-demo#Postcode"^^xsd:anyURI ;
  mim:naam           "Postcode"@nl ;
  mim:definitie      "Nederlandse postcode bestaande uit vier cijfers en twee letters."@nl ;
  mim:dataElement    ex:pc
.

ex:pc a mim:DataElement ;
  mim:identificatie  "http://example.org/mim-demo#pc"^^xsd:anyURI ;
  mim:naam           "waarde"@nl ;
  mim:definitie      "De postcodewaarde."@nl ;
  mim:type           ex:Tekst255 ;
  mim:patroon        "^[1-9][0-9]{3}[A-Z]{2}$" ;
  mim:kardinaliteit  "1..1" ;
  mim:datumOpname    "2025-09-05"^^xsd:date
.

#################################################################
# Enumeratie (Codelijst/Enumeratiewaarden)
#################################################################

ex:Geslacht a mim:Enumeratie ;
  mim:identificatie  "http://example.org/mim-demo#Geslacht"^^xsd:anyURI ;
  mim:naam           "Geslacht"@nl ;
  mim:definitie      "Lijst van mogelijke geslachten."@nl ;
  mim:waarde         ex:GeslachtM, ex:GeslachtV ;
  mim:datumOpname    "2025-09-05"^^xsd:date
.

ex:GeslachtM a mim:Enumeratiewaarde ;
  mim:identificatie  "http://example.org/mim-demo#GeslachtM"^^xsd:anyURI ;
  mim:naam           "Man"@nl ;
  mim:code           "M" ;
  mim:datumOpname    "2025-09-05"^^xsd:date
.

ex:GeslachtV a mim:Enumeratiewaarde ;
  mim:identificatie  "http://example.org/mim-demo#GeslachtV"^^xsd:anyURI ;
  mim:naam           "Vrouw"@nl ;
  mim:code           "V" ;
  mim:datumOpname    "2025-09-05"^^xsd:date
.

#################################################################
# Referentielijst (voorbeeld met externe lijst)
#################################################################

ex:LandcodeLijst a mim:Referentielijst ;
  mim:identificatie  "http://example.org/mim-demo#LandcodeLijst"^^xsd:anyURI ;
  mim:naam           "ISO 3166-1 alpha-2 landcodes"@nl ;
  mim:locatie        "https://www.iso.org/iso-3166-country-codes.html" ;
  mim:referentieElement ex:LandcodeElementNL, ex:LandcodeElementBE ;
  mim:datumOpname    "2025-09-05"^^xsd:date
.

ex:LandcodeElementNL a mim:ReferentieElement ;
  mim:identificatie  "http://example.org/mim-demo#LandcodeElementNL"^^xsd:anyURI ;
  mim:naam           "Nederland"@nl ;
  mim:type           ex:Tekst255 ;
  mim:code           "NL" ;
  mim:kardinaliteit  "1..1" ;
  mim:identificerend true ;
  mim:datumOpname    "2025-09-05"^^xsd:date
.

ex:LandcodeElementBE a mim:ReferentieElement ;
  mim:identificatie  "http://example.org/mim-demo#LandcodeElementBE"^^xsd:anyURI ;
  mim:naam           "BelgiÃ«"@nl ;
  mim:type           ex:Tekst255 ;
  mim:code           "BE" ;
  mim:kardinaliteit  "1..1" ;
  mim:identificerend true ;
  mim:datumOpname    "2025-09-05"^^xsd:date
.

#################################################################
# Objecttypen met attributen en gegevensgroep
#################################################################

ex:Persoon a mim:Objecttype ;
  mim:identificatie  "http://example.org/mim-demo#Persoon"^^xsd:anyURI ;
  mim:naam           "Persoon"@nl ;
  mim:definitie      "Een natuurlijk persoon."@nl ;
  mim:datumOpname    "2025-09-05"^^xsd:date ;
  mim:attribuut      ex:bsn, ex:naam, ex:geboortedatum, ex:geslacht ;
  mim:gegevensgroep  ex:Persoonsgegevens ;
  mim:indicatieAbstractObject false ;
  mim:populatie      "Alle ingeschreven personen in het domein." ;
  mim:kwaliteit      "Bron: bevolkingsregister." ;
  mim:toelichting    "Voorbeeldobject in het demomodel."
.

ex:Adres a mim:Objecttype ;
  mim:identificatie  "http://example.org/mim-demo#Adres"^^xsd:anyURI ;
  mim:naam           "Adres"@nl ;
  mim:definitie      "Een postadres."@nl ;
  mim:datumOpname    "2025-09-05"^^xsd:date ;
  mim:attribuut      ex:straat, ex:huisnummer, ex:postcodeAttr, ex:landcode ;
  mim:indicatieAbstractObject false
.

