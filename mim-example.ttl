#################################################################
# Relatiesoort en Relatieklasse
#################################################################

ex:woontOp a mim:Relatiesoort ;
  mim:identificatie  "http://example.org/mim-demo#woontOp"^^xsd:anyURI ;
  mim:naam           "woont op"@nl ;
  mim:definitie      "Relatie die aangeeft op welk adres een persoon woont."@nl ;
  mim:unidirectioneel true ;
  mim:bron           ex:Persoon ;
  mim:doel           ex:Adres ;
  mim:aggregatietype mim:Geen ;
  mim:kardinaliteit  "0..*" ;
  mim:datumOpname    "2025-09-05"^^xsd:date
.

# Voorbeeld relatieklasse die extra gegevens op de relatie vastlegt
ex:RelKlasseVoorbeeld a mim:Relatieklasse ;
