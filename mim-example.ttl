  mim:authentiek     skos:Concept ;
  mim:heeftTijdlijnGeldigheid false ;
  mim:indicatieMaterieleHistorie false ;
  mim:indicatieFormeleHistorie  false ;
  mim:heeftTijdlijnRegistratie  false ;
  mim:indicatieAfleidbaar false ;
  mim:indicatieClassificerend false ;
  mim:mogelijkGeenWaarde false ;
  mim:datumOpname    "2025-09-05"^^xsd:date
.

